{
   "Import Module": {
      "prefix": "imp",
      "body": [
         "import $1 from '$2'"
      ],
      "description": "Import Module"
   },
   "Import Named Export": {
      "prefix": "imn",
      "body": [
         "import { $1 } from '$2'"
      ],
      "description": "Import Named Export"
   },
   "Import useState": {
      "prefix": "ims",
      "body": [
         "import { useState } from 'react'"
      ],
      "description": "Import useState"
   },
   "Import useEffect": {
      "prefix": "ime",
      "body": [
         "import { useEffect } from 'react'"
      ],
      "description": "Import useEffect"
   },
   "Import useState and useEffect": {
      "prefix": "ima",
      "body": [
         "import { useState, useEffect } from 'react'"
      ],
      "description": "Import useState and useEffect"
   },
   "Named Function": {
      "prefix": "nfn",
      "body": [
         "const $1 = ($3) => {",
         "\t$2",
         "}"
      ],
      "description": "Create a named function"
   },
   "Anonymous Function": {
      "prefix": "anfn",
      "body": [
         "($2) => {",
         "\t$1",
         "}"
      ],
      "description": "Create an anonymous function"
   },
   "State Component Return": {
      "prefix": "slr",
      "body": [
         "export default function $1() {",
         "\treturn (",
         "\t\t<>",
         "\t\t\t$2",
         "\t\t</>",
         "\t)",
         "}"
      ],
      "description": "React Functional Component Return"
   },
   "useState": {
      "prefix": "uss",
      "body": [
         "const [${1}, set${1/(.*)/${1:/capitalize}/}] = useState($2)"
      ],
      "description": "useState Hook"
   },
   "useEffect": {
      "prefix": "uef",
      "body": [
         "useEffect(() => {",
         "\t$1",
         "}, [])"
      ],
      "description": "useEffect Hook"
   }
}